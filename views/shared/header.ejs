    <script>
      $(document).ready(function () {
        $("#id-btn-show-permittedip-chongpan").click(function (e) {
          e.preventDefault();

          $.ajax({
            type: "get",
            url: `/api/chongpan/permittedIp`,
            success: function (data) {
              const ip1 = data.address.split(",")[0];
              const ip2 = data.address.split(",")[1];
              const ip3 = data.address.split(",")[2];

              $("#id-input-chongpan-permittedip1").val(ip1);
              $("#id-input-chongpan-permittedip2").val(ip2);
              $("#id-input-chongpan-permittedip3").val(ip3);
              $("#id-modal-permittedip").modal("show");
            },
            error: function (data) {
              if (data.msg == "user not found")
                toastr["error"]("서버오류로 변경할수 없음!", "");
            },
          });
        });

        $("#id-btn-permittedip-chongpan").click(function () {
          if (
            $("#id-input-chongpan-permittedip1").val() == "" ||
            $("#id-input-chongpan-permittedip2").val() == "" ||
            $("#id-input-chongpan-permittedip3").val() == ""
          ) {
            toastr["warning"]("모든 정보를 정확히 입력하세요!", "");
            return;
          }

          let data = {
            ip:
              $("#id-input-chongpan-permittedip1").val().trim() +
              "," +
              $("#id-input-chongpan-permittedip2").val().trim() +
              "," +
              $("#id-input-chongpan-permittedip3").val().trim(),
          };

          $.ajax({
            type: "post",
            url: `/api/chongpan/permittedIp`,
            dataType: "json",
            data: data,
            success: function (data) {
              $("#id-modal-permittedip").modal("hide");
              toastr["success"]("성공적인 조작", "");
            },
            error: function (data) {
              if (data.responseJSON.msg == "server error occured")
                toastr["error"]("서버오류로 실패", "");
            },
          });
        });
      });
    </script>
