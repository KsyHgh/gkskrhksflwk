    <script>
      function date_time(id) {
          date = new Date;
          month = date.getMonth();
          year = date.getFullYear();
          months = new Array('01', '02', '03', '04', '05', '06', '07', '08', '09', '10', '11', '12');
          d = date.getDate();
          day = date.getDay();
          days = new Array('<%= __('header.date.sun') %>', '<%= __('header.date.mon') %>', '<%= __('header.date.tue') %>', '<%= __('header.date.wed') %>', '<%= __('header.date.thu') %>', '<%= __('header.date.fri') %>', '<%= __('header.date.sat') %>');
          h = date.getHours();
          if (h < 10) {
              h = "0" + h;
          }
          m = date.getMinutes();
          if (m < 10) {
              m = "0" + m;
          }
          s = date.getSeconds();
          if (s < 10) {
              s = "0" + s;
          }
          result = ' ' + year + '-' + months[month] + '-' + d + ' / ' + h + ':' + m + ':' + s + '  ' + days[day];
          document.getElementById(id).innerHTML = result;
          setTimeout('date_time("' + id + '");', '1000');
          return true;
      }
      date_time('date_time');

      function startLoading() {
          $(".loading").prop("style", "display: block;");
      }

      function endLoading() {
          $(".loading").prop("style", "display: none;");
      }
    </script>

